
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/_Layout.cshtml";
}

<h4>İstatistikler</h4>
<br />
<div class="alert alert-danger">Kategori Sayısı: @ViewBag.v1</div>
