@model List<MeyawoPortfolio.Models.Tbl_Project>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/_Layout.cshtml";
}

<h2>Proje Listesi</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>Proje ID</th>
        <th>Proje Başlığı</th>
        <th>Proje Kategorisi</th>
        <th>Github Linki</th>
        <th>Güncelle</th>
        <th>Sil</th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.ProjectID</td>
    <td>@item.Title</td>
    <td>@item.ProjectCategory</td>
    <td> <a href="♯" class="btn btn-outline-dark">Github'da Aç'</a></td>
    <td> <a href="♯" class="btn btn-outline-success">Güncelle</a></td>
    <td> <a href="♯" class="btn btn-outline-danger">Sil</a></td>
</tr>
    }
</table>
<a href="/Service/AddService" class="btn btn-outline-primary col-12">Yeni Proje Ekle!</a>

